{
  "name": "github-actions-node-versions",
  "version": "1.0.2",
  "description": "Update node_versions to LTS versions in GitHub Actions.",
  "keywords": [
    "github",
    "actions",
    "cli"
  ],
  "homepage": "https://github.com/azu/github-actions-node-versions",
  "bugs": {
    "url": "https://github.com/azu/github-actions-node-versions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/github-actions-node-versions.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "main": "lib/github-actions-node-versions.js",
  "module": "module/github-actions-node-versions.js",
  "types": "lib/github-actions-node-versions.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "github-actions-node-versions": "./bin/cmd.js"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "scripts": {
    "test": "mocha \"test/**/*.ts\"",
    "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/ module/",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/semver": "^7.3.6",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "globby": "^11.0.3",
    "meow": "^9.0.0",
    "node-version-alias": "^1.0.1",
    "semver": "^7.3.5",
    "yaml": "^2.0.0-5"
  }
}
