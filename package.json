{
  "name": "github-actions-node-versions",
  "version": "1.0.3",
  "description": "Update node_versions to LTS versions in GitHub Actions.",
  "keywords": [
    "github",
    "actions",
    "cli"
  ],
  "homepage": "https://github.com/azu/github-actions-node-versions",
  "bugs": {
    "url": "https://github.com/azu/github-actions-node-versions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/github-actions-node-versions.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./module/github-actions-node-versions.d.ts",
        "default": "./module/github-actions-node-versions.js"
      },
      "default": "./module/github-actions-node-versions.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./module/github-actions-node-versions.js",
  "module": "./module/github-actions-node-versions.js",
  "types": "./module/github-actions-node-versions.d.ts",
  "bin": {
    "github-actions-node-versions": "./bin/cmd.mjs"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "bin/",
    "module/",
    "src/"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "mocha",
    "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
    "watch": "tsc --build --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "globby": "^13.1.4",
    "meow": "^11.0.0",
    "node-version-alias": "^3.4.1",
    "semver": "^7.5.0",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@types/semver": "^7.3.13",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@1.22.19"
}
